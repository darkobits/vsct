{
  "name": "@darkobits/vsct",
  "version": "0.2.3",
  "author": "darkobits",
  "description": "A framework for creating VS Code themes and distributing them via NPM.",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/darkobits/vsct"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "vsct": "dist/cli/vsct.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "new-clean": "base clean",
    "clean": "del dist",
    "check:styles": "tslint --project tsconfig.json --format codeFrame",
    "check:types": "tsc --emitDeclarationOnly",
    "test": "jest",
    "prebuild": "run-p clean check:*",
    "build": "babel src --extensions '.ts' --out-dir dist",
    "prebump": "run-s test build",
    "bump": "standard-version",
    "bump:beta": "npm run prebump && standard-version --prerelease=beta",
    "link": "concurrently \"npm run check:types -- --outDir $(npx clean-link-dir)/dist --watch --preserveWatchOutput\" \"npm run build -- --out-dir $(npx clean-link-dir)/dist --watch --verbose\"",
    "base": "base"
  },
  "dependencies": {
    "@darkobits/import-unique": "^1.0.2",
    "@darkobits/log": "^1.1.3",
    "bottleneck": "^2.10.0",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "color": "^3.0.0",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "rc": "^1.2.8",
    "read-pkg-up": "^4.0.0",
    "traverse": "^0.6.6",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "@darkobits/clean-link": "^2.0.1",
    "@types/chalk": "^2.2.0",
    "@types/chokidar": "^1.7.5",
    "@types/color": "^3.0.0",
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/rc": "0.0.1",
    "@types/read-pkg-up": "^3.0.1",
    "@types/tempy": "^0.1.0",
    "@types/traverse": "^0.6.32",
    "@types/yargs": "^11.1.1",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "standard-version": "^4.4.0",
    "tempy": "^0.2.1",
    "tslint-xo": "^0.9.0"
  }
}
